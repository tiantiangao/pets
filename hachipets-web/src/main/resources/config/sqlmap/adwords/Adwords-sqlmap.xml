<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="Adwords">

    <typeAlias alias="adwords" type="com.gtt.pets.entity.adwords.AdwordsEntity" />

    <select id="load" parameterClass="map" resultClass="adwords" >
        <![CDATA[
            SELECT AdwordsID, TypeID, AdwordsUrl, AdwordsTitle, PicUrl, AddTime
            FROM Pets_Adwords_Type
            WHERE AdwordsID=#adwordsId# and TypeID=#typeId#
        ]]>
    </select>

</sqlMap>