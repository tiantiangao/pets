<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package name="login" namespace="/" extends="default">
    
        <action name="oAuthLoginAction" class="com.gtt.pets.web.action.oauth.OAuthLoginAction">
            <result name="success" type="redirect">${authUri}</result>
            <result name="INDEX" type="redirect">/</result>
        </action>

        <action name="tLoginAction" class="com.gtt.pets.web.action.oauth.TLoginAction">
            <result name="success" type="redirect">/</result>
        </action>

    </package>
</struts>